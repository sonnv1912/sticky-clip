{
   "compilerOptions": {
      "target": "ESNext",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "outDir": "dist",
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "resolveJsonModule": true,
      "jsx": "react-jsx"
   },
   "include": ["src/**/*", "*.d.ts"]
}
